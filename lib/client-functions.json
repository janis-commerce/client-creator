[
	["function", {
		"functionName": "MongoDBIndexCreator",
		"handler": "src/lambda/MongoDBIndexCreator/index.handler",
		"description": "MongoDB Indexes Creation Lambda",
		"timeout": 60,
		"package": {
			"include": ["schemas/mongo/**"]
		}
	}],

	["janis.eventListener", {
		"serviceName": "id",
		"entityName": "client",
		"eventName": "created",
		"mustHaveClient": false,
		"timeout": 60,
		"package": {
			"include": ["schemas/mongo/**"]
		}
	}],

	["janis.apiPost", {
		"entityName": "client",
		"authorizer": "ServiceNoClientAuthorizer",
		"timeout": 60,
		"package": {
			"include": ["schemas/mongo/**"]
		}
	}]

]